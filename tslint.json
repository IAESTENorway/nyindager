{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended", "tslint-react", "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "quotemark": [true, "double", "jsx-double"],
        "variable-name": [true, "ban-keywords", "check-format", "allow-pascal-case"],
        "no-any": true
    },
    "rulesDirectory": []
}